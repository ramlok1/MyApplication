     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        -->
        <activity
            android:name=".SplashScreen"
            android:configChanges="orientation|keyboardHidden"
            android:noHistory="true"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EncuestaActivity"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustPan|adjustResize"/>
        <activity
            android:name=".Actualizar_Preguntas"
            android:screenOrientation="landscape" />
        <activity
            android:name=".EncuestaAgregarFolio"
            android:configChanges="orientation|keyboardHidden"
            android:noHistory="true"
            android:screenOrientation="landscape" />
        <activity
            android:name=".EncuestaCupones"
            android:windowSoftInputMode="adjustPan|adjustResize"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Wizard_Main"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SeleccionarCupon"
            android:windowSoftInputMode="adjustPan|adjustResize"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".ContestarEncuesta"
            android:windowSoftInputMode="adjustPan|adjustResize"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Confirmacion"
            android:screenOrientation="landscape" />
        <activity
            android:name=".CambioCupon"
            android:screenOrientation="landscape" />
        <activity
            android:name=".NoShow"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Operacion"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AbordarSinCupon"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Abordar"
            android:screenOrientation="landscape" />
        <activity
            android:name=".CambiarPAX"
            android:screenOrientation="landscape"/>
        <activity
            android:name=".SincronizarPreguntas"
            android:noHistory="true"
            android:screenOrientation="landscape" />
    </application>

</manifest>